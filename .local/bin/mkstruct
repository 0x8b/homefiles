#!/usr/bin/env python

import fileinput

from pathlib import Path

stack = []

for line in fileinput.input():
    count = (len(line) - len(line.lstrip())) // 2

    line = line.strip()

    if line.endswith("/"):
        type = "d"
        line = line[:-1]
    else:
        type = "f"

    stack = stack[:count] + [line]

    path = Path("/".join(stack))

    if path.exists():
        continue

    if type == "f":
        path.touch() # or mode=0o766
    else:
        path.mkdir(parents=True)
